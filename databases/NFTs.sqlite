-- Create the table
CREATE TABLE NFTs (
	id INTEGER PRIMARY KEY AUTOINCREMENT,
	NFTtitle TEXT NOT NULL,
	NFTdescription TEXT NOT NULL,
	NFTprice REAL NOT NULL,
	NFTtoken TEXT NOT NULL
);

